{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["warn", {
        "singleQuote": true,
        "tabWidth": 2,
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "jsxSingleQuote": true
      }],
      "import/first": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint"
  }